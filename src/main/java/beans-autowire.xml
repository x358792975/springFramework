<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="address" class="com.selflearning.spring.test.sample2.autowire.Address"
		p:city="New York" p:street="Jewel Ave">
	</bean>

	<bean id="address2" class="com.selflearning.spring.test.sample2.autowire.Address"
		p:city="New Jercy" p:street="Washington Ave">
	</bean>
	
	<bean id="car" class="com.selflearning.spring.test.sample2.autowire.Car"
		p:brand="BMW" p:price="50000">
	</bean>

	

	<!--
	autowire: byName and byType
	Can not have both at same time
	byType bean must be unique
	Disadvantage:
	
	-->
	<bean id="person" class="com.selflearning.spring.test.sample2.autowire.Person"
		p:name="Sean" autowire="byName"></bean>
	
</beans>
